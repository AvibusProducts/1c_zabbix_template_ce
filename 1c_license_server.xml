                    </preprocessing>
                    <master_item>
                        <key>1c.licenses.info</key>
                    </master_item>
                </item>
                <item>
                    <name>[1С/Лицензии] Сводная информация</name>
                    <key>1c.licenses.info</key>
                    <delay>1h</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>[1С] Информация</name>
                        </application>
                        <application>
                            <name>[1С] Программные лицензии</name>
                        </application>
                    </applications>
