                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>[1С] Программные лицензии</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>{#FILE}:\w+:(.*)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>1c.licenses.info</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{date()}&gt;{last()}</expression>
                                    <name>[1С/Лицензии] Истек срок действия лицензии ({#FILE})</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>[1С/Лицензии] Тип лицензии ({#FILE})</name>
                            <type>DEPENDENT</type>
                            <key>1c.license.type[{#FILE}]</key>
